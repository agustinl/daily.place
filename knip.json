{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "entry": [
    "src/pages/**/*.{ts,tsx}",
    "src/pages/api/**/*.{ts,tsx}"
  ],
  "project": [
    "src/**/*.{ts,tsx}"
  ],
  "ignore": [
    "**/*.d.ts",
    "**/*.test.{ts,tsx}",
    "**/*.spec.{ts,tsx}",
    "public/**",
    "node_modules/**",
    ".next/**"
  ],
  "ignoreDependencies": [
    "@types/*"
  ],
  "ignoreExportsUsedInFile": true
}

